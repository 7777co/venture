{
    "name": "sassnowski/venture",
    "description": "A package to manage complex workflows built on top of Laravel's queue.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kai Sassnowski",
            "email": "me@kai-sassnowski.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/container": "^7.0|^8.0",
        "illuminate/bus": "^7.0|^8.0",
        "illuminate/queue": "^7.0|^8.0",
        "ramsey/uuid": "^4.0",
        "opis/closure": "^3.5",
        "illuminate/events": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sassnowski\\Venture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sassnowski\\Venture\\VentureServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^0.3.8",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4",
        "spatie/phpunit-watcher": "^1.22",
        "pestphp/pest-plugin-laravel": "^0.3.0"
    }
}
